    <title>Show details</title>
    <link rel="stylesheet" href="bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="css/Login-Form-Dark.css" />
    <link rel="stylesheet" href="css/styles.css" />
  </head>

  <body>
    <div class="login-dark">
        <div class="table-responsive" style="margin:26px 0px;">
          <% if(DBdata.access == 1){%>
          <table class="table">
            <thead>
              <tr>
                <th>name</th>
                <th>age</th>
                <th>airline</th>
              </tr>
            </thead>
            <tbody>
              <%for(var i=0;i < DBdata.view.pilots.length; i++){%>
              <tr>
                <td>DBdata.view.pilots.name</td>
                <td>DBdata.view.pilots.age</td>
                <td>DBdata.view.pilots.airline</td>
              </tr>
              <%}%>
            </tbody>
          </table>
          <%}%>
        </div>
      </form>
    </div>
    <script src="js/jquery.min.js"></script>
    <script src="bootstrap/js/bootstrap.min.js"></script>
  </body>
</html>
